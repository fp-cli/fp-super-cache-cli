{
    "name": "fp-cli/fp-super-cache-cli",
    "type": "fp-cli-package",
    "description": "Add a `fp super-cache` command to support the FP Super Cache plug-in",
    "keywords": [
        "fp-cli",
        "super-cache",
        "cache"
    ],
    "homepage": "https://github.com/fp-cli/fp-super-cache-cli",
    "license": "MIT",
    "authors": [
        {
            "name": "FP-CLI Team",
            "homepage": "http://github.com/fp-cli",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "fp-cli/fp-cli": "^2"
    },
    "require-dev": {
        "fp-cli/extension-command": "^2",
        "fp-cli/fp-cli-tests": "^2.0.7"
    },
    "config": {
        "platform": {
            "php": "5.4"
        },
        "process-timeout": 7200,
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "files": [
            "fp-super-cache-cli.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "behat": "run-behat-tests",
        "lint": "run-linter-tests",
        "phpcs": "run-phpcs-tests",
        "phpunit": "run-php-unit-tests",
        "prepare-tests": "install-package-tests",
        "test": [
            "@lint",
            "@phpcs",
            "@phpunit",
            "@behat"
        ]
    }
}
